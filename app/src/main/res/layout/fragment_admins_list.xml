<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".view.AdminsListFragment" android:background="#424242">
    <ProgressBar
            style="?android:attr/progressBarStyle"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/progressBar3"
            android:layout_gravity="center"
            android:indeterminate="true"
            android:indeterminateTintMode="src_atop"
            android:indeterminateTint="@color/white"
            android:visibility="gone"
    />
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/adminsList_rcView"
        android:layout_width="match_parent" android:layout_height="match_parent"
        android:nestedScrollingEnabled="true" android:overScrollMode="never" tools:itemCount="14"
        tools:listitem="@layout/lite_player_item">
    </androidx.recyclerview.widget.RecyclerView>
</FrameLayout>